#MembershipCarousel.carousel.slide
	.carousel-inner
		.item.active
			= link_to image_tag('bell-header1.jpg'), root_path, class: "img-responsive"
			.container
				.carousel-caption
					%h1 You Know You Need It?
					%p.lead Membership gets you access to amazing professionals in your area ready to serve!

		.item
			= link_to image_tag('chef-banner1.jpg'), root_path, class: "img-responsive"
			.container
				.carousel-caption
					%h1 Massage?
					%p.lead A one hour massage is included

		.item
			= link_to image_tag('massage-banner1.jpg'), root_path, class: "img-responsive"
			.container
				.carousel-caption 
					%h1 Yoga?
					%p.lead Book two yoga sessions.


		.item
			= link_to image_tag('hair-banner1.jpg'), root_path, class: "img-responsive"
			.container
				.carousel-caption
					%h1 Dinner?
					%p.lead Get a chef in to design, prepare and serve an amazing meal.

	%a.left.carousel-control{"data-slide" => "prev", :href => "#myCarousel", :role => "button"}
		%span.glyphicon.glyphicon-chevron-left{"aria-hidden" => "true"}
		%span.sr-only Previous
	%a.right.carousel-control{"data-slide" => "next", :href => "#myCarousel", :role => "button"}
		%span.glyphicon.glyphicon-chevron-right{"aria-hidden" => "true"}
		%span.sr-only Next

- @vendor_list.each do |vendor_type, vendor_items|
	%h2= vendor_type
	#vendor_memberscroll.container-fluid
		.wrapper
			.scroller
				- vendor_items.each_with_index do |vendor_item, i|
					%a{ href: (url_for [vendor_item])}
						.box.panel.panel-default
							.panel-heading
								%h2= vendor_item.name
							.panel-body
								%img.img-responsive.img-rounded{alt: "Profile Avatar", src: vendor_item.avatar.url(:medium)}


#welcome_contact.container-fluid
	.wrapper
		.row
			%link{:href => "//cdn-images.mailchimp.com/embedcode/classic-10_7.css", :rel => "stylesheet", :type => "text/css"}/
			:css
				#mc_embed_signup{background:#fff; clear:left; font:14px Helvetica,Arial,sans-serif; }

			#mc_embed_signup
				%form#mc-embedded-subscribe-form.validate{:action => "//pamperedup.us12.list-manage.com/subscribe/post?u=fd9fd116c23d75e6992fb7233&id=c9890c302f", :method => "post", :name => "mc-embedded-subscribe-form", :novalidate => "", :target => "_blank"}
					#mc_embed_signup_scroll
						%h2 Subscribe to our mailing list
						.indicates-required
							%span.asterisk *
							indicates required
						.mc-field-group
							%label{:for => "mce-EMAIL"}
								Email Address
								%span.asterisk *
							%input#mce-EMAIL.required.email{:name => "EMAIL", :type => "email", :value => ""}/
						.mc-field-group
							%label{:for => "mce-FNAME"} First Name
							%input#mce-FNAME{:name => "FNAME", :type => "text", :value => ""}/
						.mc-field-group
							%label{:for => "mce-ZIP"}
								Zip
								%span.asterisk *
							%input#mce-ZIP.required{:name => "ZIP", :type => "text", :value => ""}/
						#mce-responses.clear
							#mce-error-response.response{:style => "display:none"}
							#mce-success-response.response{:style => "display:none"}

						%div{"aria-hidden" => "true", :style => "position: absolute; left: -5000px;"}
							%input{:name => "b_fd9fd116c23d75e6992fb7233_c9890c302f", :tabindex => "-1", :type => "text", :value => ""}/
						.clear
							%input#mc-embedded-subscribe.button{:name => "subscribe", :type => "submit", :value => "Subscribe"}/
					%script{:src => "//s3.amazonaws.com/downloads.mailchimp.com/js/mc-validate.js", :type => "text/javascript"}

