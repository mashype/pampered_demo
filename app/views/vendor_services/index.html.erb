<p id="notice"><%= notice %></p>

<h1>Listing Vendor Services</h1>

<table>
  <thead>
    <tr>
      <th>Vendor</th>
      <th>Service</th>
      <th>Notes</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vendor_services.each do |vendor_service| %>
      <tr>
        <td><%= vendor_service.vendor_id %></td>
        <td><%= vendor_service.service_id %></td>
        <td><%= vendor_service.notes %></td>
        <td><%= link_to 'Show', vendor_service %></td>
        <td><%= link_to 'Edit', edit_vendor_service_path(vendor_service) %></td>
        <td><%= link_to 'Destroy', vendor_service, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vendor service', new_vendor_service_path %>
